@page
@model Narciarze_v_2.Pages.Strefa_Administracji.Administrator.AdministratorModel
@{
    ViewData["Title"] = "ADMIN";
    Layout = "_Layout_Admin";
    if (HttpContext.Session.GetInt32("_prac_uid") != 3)
    {
        Response.Redirect("../../Index");
    }
    var imie = HttpContext.Session.GetString("_prac_imie");
    var nazwisko = HttpContext.Session.GetString("_prac_nazwisko");
}
<div class="text-center">
    <section>
        <h2>Witaj @imie @nazwisko</h2>
        
    </section>
</div>
